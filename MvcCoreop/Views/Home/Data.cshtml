@model MvcCoreop.Models.DataViewModel


<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
            </tr>
            @foreach (var record in Model.Datas)
            {
                <tr>
                    <td>@(record.Seq)</td>
                    <td>@(record.Category)</td>
                    <td>@(record.Date)</td>
                    <td>@(record.Price)</td>
                </tr>
            }
        </table>
    </div>
</div>

